{
  "name": "source-map-loader",
  "version": "5.0.0",
  "description": "extracts inlined source map and offers it to webpack",
  "keywords": [
    "webpack"
  ],
  "homepage": "https://github.com/webpack-contrib/source-map-loader",
  "bugs": "https://github.com/webpack-contrib/source-map-loader/issues",
  "repository": "webpack-contrib/source-map-loader",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/webpack"
  },
  "license": "MIT",
  "author": "Tobias Koppers @sokra",
  "main": "dist/cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "commitlint": "commitlint --from=master",
    "security": "npm audit",
    "lint:prettier": "prettier --list-different .",
    "lint:code": "eslint --cache .",
    "lint:spelling": "cspell \"**/*.*\"",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "fix:code": "npm run lint:code -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "npm-run-all -l fix:code fix:prettier",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --collectCoverageFrom=\"src/**/*.js\" --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "prepare": "husky && npm run build",
    "release": "standard-version"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "source-map-js": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@eslint/js": "^9.32.0",
    "@eslint/markdown": "^7.0.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "cross-env": "^7.0.3",
    "cspell": "^8.10.0",
    "del-cli": "^6.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-webpack": "^4.4.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^52.0.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unicorn": "^60.0.0",
    "husky": "^9.1.3",
    "jest": "^30.0.0",
    "lint-staged": "^15.2.7",
    "memfs": "^4.9.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "standard-version": "^9.5.0",
    "typescript-eslint": "^8.38.0",
    "webpack": "^5.92.1"
  },
  "peerDependencies": {
    "webpack": "^5.72.1"
  },
  "engines": {
    "node": ">= 18.12.0"
  }
}
